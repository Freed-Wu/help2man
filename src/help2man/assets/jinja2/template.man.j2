.\" DO NOT MODIFY THIS FILE!  It was generated by help2man {{ help2man_version }}.
.TH {{ prog.upper() }} "{{ section }}" "{{ date }}" "{%- if source -%}{{ source }} {%- endif -%}{{ prog }} {{ version }}" "{{ manual }}"
.SH {{ name.upper() }}
{{ prog }} \- {{ description }}
.SH SYNOPSIS
\&{{ synopsis }}
{% if description -%}
.SH DESCRIPTION
\&{{ description }}
{% endif -%}
{% for s in sections -%}
.SH {{ s.title.upper() }}
{% for c in s.contents -%}
{% if c.name -%}
.TP
\&{{ c.name }}
\&{{ c.description }}
{% else -%}
.IP
\&{{ c.description }}
{% endif -%}
{% endfor -%}
{% endfor -%}
{% if author %}
.SH AUTHOR
Written by {{ author }}
{% endif %}
{% if bug %}
.SH "REPORTING BUGS"
Report bugs to {{ bug }}
{% endif %}
{% if copyright %}
.SH COPYRIGHT
Copyright \(co {{ copyright }}
{% endif %}
{% if info_page -%}
.SH "SEE ALSO"
The full documentation for
.B {{ prog }}
is maintained as a Texinfo manual.  If the
.B info
and
.B {{ prog }}
programs are properly installed at your site, the command
.IP
.B info {{ info_page }}
.PP
should give you access to the complete manual.
{%- endif -%}
